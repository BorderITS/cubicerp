<?xml version="1.0" ?>
<openerp>
    <data>

        <record model='ir.ui.view' id='view_budget_struct_tree'>
			<field name='name'>budget.struct.tree.inherit.budget</field>
			<field name='model'>budget.struct</field>
			<field name='inherit_id' ref='budget.view_budget_struct_tree'/>
			<field name='arch' type='xml'>
                <xpath expr="//field[@name='type']" position="after">
                    <field name="available"/>
                    <field name="committed"/>
                    <field name="provision"/>
                    <field name="paid"/>
                </xpath>
			</field>
		</record>

        <record model='ir.ui.view' id='view_budget_struct_form'>
			<field name='name'>budget.struct.form.inherit.budget</field>
			<field name='model'>budget.struct</field>
			<field name='inherit_id' ref='budget.view_budget_struct_form'/>
			<field name='arch' type='xml'>
                <xpath expr="//group[@name='one']" position="after">
                    <group name="two">
                        <field name="available"/>
                        <field name="committed"/>
                        <field name="provision"/>
                        <field name="paid"/>
                    </group>
                </xpath>
			</field>
		</record>

    </data>
</openerp>


